  <data name="xrTableCell28.ExpressionBindings" xml:space="preserve">
    <value>Iif(IsNullOrEmpty([A]),'N.E.' , 
Iif([Kategori]='Yasaklı','FAIL', Iif(Iif(IsNullOrEmpty([Noael]),'N/A',([Noael]/([Miktar]*[A]*[DaP]/100*1/100)))
='N/A','SUITABLE' , Iif((Iif(IsNullOrEmpty([Noael]),'N/A',([Noael]/([Miktar]*[A]*[DaP]/100*1/100)))
)&gt;100,'SUITABLE' , 'FAIL')
)

))</value>
